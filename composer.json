    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "^1.6",
        "bryanjhv/slim-session": "~3.0",
        "crell/api-problem": "^2.0",
        "doctrine/dbal": "^2.9",
        "gofabian/negotiation-middleware": "^0.1.2",
        "imagine/imagine": "^0.7.1",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.13.0",
        "league/tactician": "^1.0",
        "lstrojny/functional-php": "^1.6",
        "micheh/psr7-cache": "^0.5.0",
        "monolog/monolog": "^1.17",
        "pagerfanta/pagerfanta": "^2.0",
        "phpmailer/phpmailer": "^6.6",
        "phpoffice/phpspreadsheet": "^1.9",
        "robmorgan/phinx": "^0.9.2",
        "slim/php-view": "^2.2",
        "slim/slim": "^3.11",
        "slimphp-api/slim-oauth": "^2.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/options-resolver": "^4.2",
        "symfony/yaml": "^4.2",
        "tuupola/base62": "^1.0",
        "tuupola/cors-middleware": "^0.8.0",
        "tuupola/slim-basic-auth": "^3.2",
        "tuupola/slim-jwt-auth": "^3.1",
