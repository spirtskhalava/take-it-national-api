{
    "name": "2amigos/take-it-national-api",
    "description": "Slim 3 + Zend Table Gateway + Monolog API skeleton",
    "type": "project",
    "keywords": [
        "slim",
        "api",
        "rest",
        "take-it-national"
    ],
    "homepage": "https://gitlab.com/2amigos/take-it-national",
    "license": "MIT",
    "authors": [
        {
            "name": "2amigos.us",
            "email": "hola@2amigos.us",
            "homepage": "https://2amigos.us/",
            "role": "Developer"
        }
