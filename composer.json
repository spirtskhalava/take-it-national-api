        "zendframework/zend-inputfilter": "^2.8",
        "zendframework/zend-uri": "^2.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10@dev",
        "overtrue/phplint": "^1.0",
        "phpunit/phpunit": "^7.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
